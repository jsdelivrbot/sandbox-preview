<!DOCTYPE html>
<html>
<head>
<!--  <% include ../partials/header.ejs %> -->
<style>
    table, th, td {
        border: 1px solid black;
        text-align: center;
    }
</style>
</head>

<body>

<!-- <% include ../partials/nav.ejs %> -->

<div class="container">
<h2>Database Results</h2>

<div>
  <table>
    <tr>
        <th>Id</th>
        <th>Release Number</th>
        <th>Release Name</th>
        <th>Org Name</th>
        <th>Org Type</th>
        <th>Region</th>
    </tr>
    <% results.forEach(function(r) { %>
    <tr>
        <td><%= r.id %></td>
        <td><%= r.internal_rel_name %></td>
        <td><%= r.external_rel_name %></td>
        <td><%= r.org_id %></td>
        <td><%= r.org_type %></td>
        <td><%= r.org_region %></td>
    </tr>    
    <% }); %>
  </table>
</div>

</div>

</body>
</html>
