<!DOCTYPE html>
<html>
<head>
<script type="text/javascript"
src="https://code.jquery.com/ui/1.10.2/jquery-ui.min.js"
integrity="sha256-FgiaQnQazF/QCrF9qSvpRY6PACn9ZF8VnlgqfqD1LsE="
crossorigin="anonymous"></script>
<script type="text/javascript" src="../../public/js/typeahead.bundle.js"></script>
<script type="text/javascript" src="../../public/js/the-basics.js"></script>
<style>
    table, th, td {
        border: 1px solid black;
        text-align: center;
    }
</style>
</head>

<body>

<div class="container">
<h2>Autocomplete Typeahead Test Page</h2>

<div id="the-basics">
    <input class="typeahead" type="text" placeholder="States of USA">
</div>

<br />

<div>
  <table>
    <tr>
        <th>Id</th>
        <th>Release Number</th>
        <th>Release Name</th>
        <th>Org Name</th>
        <th>Org Type</th>
    </tr>
    <% results.forEach(function(r) { %>
    <tr>
        <td><%= r.id %></td>
        <td><%= r.internal_rel_name %></td>
        <td><%= r.external_rel_name %></td>
        <td><%= r.org_id %></td>
        <td><%= r.org_type %></td>
    </tr>    
    <% }); %>
  </table>
</div>

</div>

</body>
</html>
